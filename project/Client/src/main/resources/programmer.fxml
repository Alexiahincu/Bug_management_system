<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.client.gui.ProgrammerController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button fx:id="btnShowAll" onAction="#loadData" text="Show all bugs" />

    <Label fx:id="welcomeText"/>
    <ListView fx:id="listBug" />

    <Button fx:id="btnSolve" onAction="#btnSolveBugPressed" text="Solve bugs" />

    <Button fx:id="btnLogOut" onAction="#btnLogOutPressed" text="Log out" />

    <Button fx:id="btnLink" onAction="#getInvLink" text="Get invitation link!" />



</VBox>

